@model IEnumerable<Biblioteca.Models.Prestamo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fecha_Prestamo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descripcion_Prestamo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Libros_Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Docete_Id)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha_Prestamo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion_Prestamo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Libros_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Docete_Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Codigo_Prestamo">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Codigo_Prestamo">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Codigo_Prestamo">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
